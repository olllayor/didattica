{
    "version": 2,
    "builds": [
      {
        "src": "config/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "headers": {
          "cache-control": "s-maxage=31536000, immutable"
        },
        "dest": "/static/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "/media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "config/wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "config.settings"
    },
    "build": {
      "env": {
        "DJANGO_SETTINGS_MODULE": "config.settings"
      }
    }
  }
  